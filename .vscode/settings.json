{
  "python.formatting.provider": "black",
  "jupyter.kernels.filter": [
    {
      "path": "/opt/conda/bin/python",
      "type": "pythonEnvironment"
    },
  ],
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": ".env*",
        "isAsync": true,
        "cmd": "/scripts/create-env-example.sh"
      },
    ]
  },
}
